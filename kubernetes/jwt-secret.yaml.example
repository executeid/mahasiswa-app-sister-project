apiVersion: v1
kind: Secret
metadata:
  name: jwt-secret
type: Opaque
data:
  JWT_SECRET_KEY: <base64_encoded_jwt_secret_key>
# Note: Replace <base64_encoded_jwt_secret_key> with the actual base64 encoded value.
# You can generate this value using the command:
# echo -n your_jwt_secret_key | base64
